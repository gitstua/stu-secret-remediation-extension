{
    "name": "stu-secret-remediation-extension",
    "displayName": "Stu Secret Remediation Extension",
    "description": "An extension to get secret alerts from a GitHub repository",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "activationEvents": [
        "onCommand:extension.getSecretScanningAlerts",
        "onCommand:extension.setGitHubToken"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.getSecretScanningAlerts",
                "title": "Get Secret Scanning Alerts from GitHub Repo"
            },
            {
                "command": "extension.setGitHubToken",
                "title": "Set GitHub Token"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^14.14.6",
        "@vscode/vsce": "3.2.1",
        "typescript": "^4.0.3",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "axios": "^0.21.1"
    }
}
